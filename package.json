{
  "name": "generate-eslint",
  "description": "Generate a `.eslintrc.json` or `.eslintignore` file as part of a larger build workflow. This generator can be used as a sub-generator or plugin inside other generators.",
  "version": "0.1.2",
  "homepage": "https://github.com/generate/generate-eslint",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-eslint",
  "bugs": {
    "url": "https://github.com/generate/generate-eslint/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "LICENSE",
    "README.md",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "is-valid-app": "^0.1.0"
  },
  "devDependencies": {
    "delete": "^0.3.2",
    "generate": "^0.7.0",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "boilerplate",
    "build",
    "cli",
    "cli-app",
    "command-line",
    "config",
    "create",
    "dev",
    "development",
    "eslint",
    "eslintignore",
    "file",
    "framework",
    "front",
    "frontend",
    "fs",
    "generate",
    "generategenerator",
    "generateplugin",
    "generator",
    "init",
    "initialize",
    "new",
    "plugin",
    "project",
    "projects",
    "rc",
    "runtime",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "template",
    "templates",
    "webapp",
    "write",
    "yeoman",
    "yo"
  ],
  "verb": {
    "toc": false,
    "layout": "generator",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "generate",
        "generate-file",
        "generate-mocha",
        "generate-node"
      ]
    },
    "reflinks": [
      "eslint",
      "generate",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
