{
  "name": "generate-eslint",
  "description": "Generate a new `.eslintrc.json` or `.eslintignore` file from a pre-defined or user-defined template. Can be used from the command line when installed globally, or as a plugin in your own generator.",
  "version": "0.2.1",
  "homepage": "https://github.com/generate/generate-eslint",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "generate/generate-eslint",
  "bugs": {
    "url": "https://github.com/generate/generate-eslint/issues"
  },
  "license": "MIT",
  "files": [
    "generator.js",
    "index.js",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "is-valid-app": "^0.3.0"
  },
  "devDependencies": {
    "delete": "^1.0.1",
    "generate": "^0.14.0",
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.4.2"
  },
  "keywords": [
    "boilerplate",
    "build",
    "cli",
    "cli-app",
    "command-line",
    "config",
    "create",
    "dev",
    "development",
    "eslint",
    "eslintignore",
    "file",
    "framework",
    "front",
    "frontend",
    "fs",
    "generate",
    "generate-generator",
    "generate-plugin",
    "generategenerator",
    "generateplugin",
    "generator",
    "init",
    "initialize",
    "new",
    "plugin",
    "project",
    "projects",
    "rc",
    "runtime",
    "scaffold",
    "scaffolder",
    "scaffolding",
    "template",
    "templates",
    "webapp",
    "write",
    "yeoman",
    "yo"
  ],
  "verb": {
    "toc": {
      "method": "preWrite"
    },
    "layout": "generator",
    "sections": {
      "file": "readme",
      "taskExamples": [
        "eslintrc",
        "ignore"
      ]
    },
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "generate",
        "generate-file",
        "generate-mocha",
        "generate-node"
      ]
    },
    "reflinks": [
      "assemble",
      "base",
      "eslint",
      "generate",
      "generate-dest",
      "gulp",
      "verb",
      "verb-readme-generator"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
